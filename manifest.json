{
  "manifest_version": 3,
  "version": "1.0",
  "name": "Div To Chrome",
  "description": "Creates a list from divs listed on the page",
  "permissions": ["scripting", "activeTab", "tabs"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
